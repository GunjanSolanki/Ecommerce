
<%= form_with url: sessions_path, scope: :session, local: true do |form| %>
  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </p>
  <p>
    <%= form.label :password %><br>
    <%= form.text_field :password %>
  </p>
  <p>
    <%= form.submit :LOGIN %><br>
  </p>
<% end %>
